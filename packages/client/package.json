  },
  "dependencies": {
    "@hello/common": "workspace:^1.0.0",
    "antd": "^4.22.7",
    "axios": "^0.27.2",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "socket.io-client": "^4.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "less": "^4.1.3",
    "sass": "^1.53.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
